{
    "name": "bahricanli/netgsm-php",
    "description": "A PHP client library for Netgsm API",
    "homepage": "https://github.com/bahricanli/netgsm",
    "version": "1.0.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bahri Meriç Canlı",
            "email": "bahri@bahri.info",
            "homepage": "https://www.bahri.info",
            "role": "Developer"
        },
        {
            "name": "Hilmi Erdem KEREN",
            "email": "erdemkeren@gmail.com",
            "homepage": "http://www.erdemkeren.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "~7.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "BahriCanli\\Netgsm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BahriCanli\\Netgsm\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
